<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:src="@mipmap/chinazhongche"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_imageview" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标定值："
        android:textSize="18sp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:layout_toEndOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:id="@+id/textView" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_biaodingzhi"
        android:text="biaodingzhi"
        android:textSize="18sp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuidazhi"
        android:layout_alignStart="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuidazhi" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="水平最大值："
        android:textSize="18sp"
        android:layout_below="@+id/textView"
        android:layout_toRightOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:layout_toEndOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:id="@+id/textView2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuidazhi"
        android:text="shuipingzuidazhi"
        android:textSize="18sp"
        android:layout_above="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuixiaozhi"
        android:layout_alignLeft="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuixiaozhi"
        android:layout_alignStart="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuixiaozhi" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="水平最小值："
        android:textSize="18sp"
        android:layout_below="@+id/textView2"
        android:layout_toRightOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:layout_toEndOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:id="@+id/textView3" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_shuipingzuixiaozhi"
        android:text="shuipingzuixiaozhi"
        android:textSize="18sp"
        android:layout_above="@+id/textView7"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="垂直最大值："
        android:textSize="18sp"
        android:layout_below="@+id/textView3"
        android:layout_toRightOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:layout_toEndOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:id="@+id/textView7" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_chuizhizuidazhi"
        android:text="chuizhizuidazhi"
        android:textSize="18sp"
        android:layout_above="@+id/identity_num_result_main_returnfrompc_activity_item_chuizhizuixiaozhi"
        android:layout_alignLeft="@+id/identity_num_result_main_returnfrompc_activity_item_chuizhizuixiaozhi"
        android:layout_alignStart="@+id/identity_num_result_main_returnfrompc_activity_item_chuizhizuixiaozhi" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="垂直最小值："
        android:textSize="18sp"
        android:layout_below="@+id/textView7"
        android:layout_toRightOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:layout_toEndOf="@+id/identity_num_result_main_returnfrompc_activity_item_imageview"
        android:id="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/identity_num_result_main_returnfrompc_activity_item_chuizhizuixiaozhi"
        android:text="chuizhizuixiaozhi"
        android:textSize="18sp"
        android:layout_alignTop="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />


</RelativeLayout>
